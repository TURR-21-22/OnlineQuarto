<%- include('../header') %>
<div class="header">
    <div class="header-logo"><img src = "/media/QUARTO-LOGO.svg" alt="QUARTO"/></div>    
    <div class="header-menu">
        <ul class="header-menu-list">
            <li class="header-menu-item">
                <form id="lobby-link" action="/lobby" method="get">
                    <button class="button button_lobby" type="submit">lobby</button>
                </form>
            </li>
            <li class="header-menu-item">
                <form id="highscore-link" action="/highscore" method="get">
                    <button class="button button_highscore" type="submit">highscore</button>
                </form>
            </li>
            <li class="header-menu-item">
                <form id="help-link" action="/help" method="get">
                    <button class="button button_help" type="submit">help</button>
                </form>
            </li>
            <li class="header-menu-item">
                <form id="about-link" action="/about" method="get">
                    <button class="button button_about" type="submit">about</button>
                </form>
            </li>
        </ul>
    </div>
</div>